<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<!-- 连接属性信息,可选 
	<properties resource="localDB.properties" />-->
	
	<!-- 路径,可选 -->
	<classPathEntry location="D:/Java/maven/repo/mysql/mysql-connector-java/6.0.6/mysql-connector-java-6.0.6.jar" />

	<context id="Mysql" targetRuntime="MyBatis3" defaultModelType="flat">
		<!-- 可选 -->
		<property name="autoDelimitKeywords" value="true"/>
		<property name="beginningDelimiter" value="`"/>
		<property name="endingDelimiter" value="`"/>

		<!-- 可选 -->
		<commentGenerator>
			<!-- 阻止生成注释 --> 
			<property name="suppressAllComments" value="true" />
			<!-- 阻止生成注释时间戳 -->
			<property name="supprssDate" value="true" />
		</commentGenerator>
	
		<jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
						connectionURL="jdbc:mysql://localhost:3306/ijjg_test?serverTimezone=UTC" 
						userId="root" password="mypass">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- 必选,包名,路径 -->
		<javaModelGenerator targetPackage="org.fictio.askPro.pojo"
			targetProject="D:\Java\workspace\askPro\src\main\java">
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!--要生成的 Mapper.xml 文件位置-->
		<sqlMapGenerator targetPackage="org.fictio.askPro.mapping"
			targetProject="D:\Java\workspace\askPro\src\main\java" />

		<!-- 生成mapper接口 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="org.fictio.askPro.dao"
			targetProject="D:\Java\workspace\askPro\src\main\java">
		</javaClientGenerator>
